{
    "manifest_version": 2,
    "name": "Youtube mp3 downloader",
    "version": "0.0.1",
    "description": "Extract youtube's audio as mp3",
    "author": "Mariano Heller",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "http://www.youtube.com/*"
            ],
            "js": [ "content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}